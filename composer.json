{
    "name": "irajtaghlidi/laravel-asanak-sms",
    "description": "Asanak.ir Notifications channel for Laravel 5.6.",
    "keywords": ["laravel", "notifications", "asanak.ir", "sms"],
    "homepage": "https://github.com/irajtaghlidi/laravel-asanak-sms",
    "license": "MIT",
    "version": "1.0.0-dev",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Iraj Taghlidi",
            "role": "Developer",
            "email": "irajtaghlidi@gmail.com",
            "homepage": "https://github.com/irajtaghlidi"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AsanakSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AsanakSms\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\AsanakSms\\AsanakSmsServiceProvider"
            ]
        }
    }
}
